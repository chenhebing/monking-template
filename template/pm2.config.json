{
    "apps": [{
        "name": "monking-demo",
        "script": "index.js",
        "env": {
            "NODE_ENV": "production"
        },
        "node_args": "--harmony",
        "instances": 0,
        "exec_mode": "cluster",
        "max_memory_restart": "1G",
        "watch": false,
        "merge_logs": true,
        "log_date_format": "YYYY-MM-DD HH:mm Z",
        "error_file": "../logs/error.log",
        "out_file": "../logs/out.log"
    }]
}
